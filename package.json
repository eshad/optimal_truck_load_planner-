  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:start": "npm run build && pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:dev": "pm2 start ecosystem.dev.config.js",
    "pm2:dev:stop": "pm2 stop ecosystem.dev.config.js",
    "pm2:dev:restart": "pm2 restart ecosystem.dev.config.js",
    "pm2:dev:delete": "pm2 delete ecosystem.dev.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:logs:live": "pm2 logs --lines 100 --raw",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 list",
    "pm2:list": "pm2 list",
    "pm2:info": "pm2 describe backend-dev || pm2 describe backend",
    "postinstall": "node -e \"const fs=require('fs');const f='node_modules/@nestjs/cli/lib/runners/abstract.runner.js';if(fs.existsSync(f)){let c=fs.readFileSync(f,'utf8');c=c.replace(/shell:\\\\s*isWindows/g,'shell: true');fs.writeFileSync(f,c);}\""
  },
